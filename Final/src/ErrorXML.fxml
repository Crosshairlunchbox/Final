<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>


<DialogPane prefHeight="97.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.60" 
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ErrorXMLController">
   <header>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Warning!">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </header>
   <content>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextArea prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="continueButton" mnemonicParsing="false" prefHeight="39.0"
                    prefWidth="137.0" text="Continue" />
         </children>
      </HBox>
   </content>
</DialogPane>
